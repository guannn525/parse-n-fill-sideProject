# PARSE-N-FILL Environment Variables
# Copy this file to .env and fill in your values

# Required: Google AI API Key for Gemini
# Get your key at https://aistudio.google.com/apikey
GOOGLE_GENERATIVE_AI_API_KEY=your_google_api_key_here

# Optional: Override default Gemini model
# Options: gemini-3-flash-preview (default), gemini-2.0-pro-exp (complex docs)
# GEMINI_MODEL=gemini-3-flash-preview

# Optional: Enable debug logging
# LOG_LEVEL=debug

# Optional: Maximum file size in MB (default: 32)
# MAX_FILE_SIZE_MB=32

# Optional: Request timeout in milliseconds (default: 60000)
# REQUEST_TIMEOUT_MS=60000
